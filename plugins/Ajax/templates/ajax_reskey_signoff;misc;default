__name__
ajax_reskey_signoff
__template__
[% reskey = Slash.getObject('Slash::ResKey');
   IF !reskey_resource;
   	reskey_resource = 'ajax_admin';
   END;
   rkey = reskey.key(reskey_resource, { nostate => 1 });
   IF rkey.create;
   	PROCESS reskey_tag;
   END;

   # don't carry over to subsequent calls
   reskey_resource = '';
   reskey_name = ''; %]
