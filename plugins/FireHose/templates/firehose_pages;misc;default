[% IF page %] <a href="[% gSkin.rootdir %]/[% fh_page %]?page=[% page - 1 %][% param_str %]"><b>&lsaquo;</b> [% page %]</a>[% END %]
[% IF page || num_items == options.limit %]<a class="fh_cur_page" href="[% gSkin.rootdir %]/[% fh_page %]?page=[% page %][% param_str %]"><strong>[% page + 1 %]</strong></a>[% END %]
[% IF num_items == options.limit %]
	<a href="[% gSkin.rootdir %]/[% fh_page %]?page=[% page + 1%][% param_str %]">[% page + 2 %] <b>&rsaquo;</b></a>
[% END %]
