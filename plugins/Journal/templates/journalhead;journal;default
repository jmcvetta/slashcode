	});
ELSIF useredit && useredit.uid != user.uid;
	Slash.createMenu("users", {
		tab_selected => "otheruser",
		style => "tabbed",
		justify => "right"
	});
	PROCESS user_titlebar width => "100%" tab_selected => "journal";
	Slash.createMenu("journal", {
		style => "oldstyle"
	});
ELSE;
	Slash.createMenu("users", {
		tab_selected => "me",
		style => "tabbed",
		justify => "right",
	});
	PROCESS user_titlebar width => "100%" tab_selected => "journal";
	Slash.createMenu("journal", {
