__template__

<TR[% IF valign %] VALIGN="[% valign %]"[% END %]>
<!-- maintable_tr -->

[%
min_cols = 999;
FOR rowcheck = user.state.mt.cols;
	NEXT IF loop.first;
	IF rowcheck < min_cols;
		min_cols = rowcheck;
		user.state.mt.curcol = loop.index;
	END;
END
%]

